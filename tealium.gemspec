Gem::Specification.new do |s|
  s.name = "tealium"
  s.version = "1.0.2"

  s.authors = ["Tealium"]
  s.date = %q{2019-01-31}
  s.summary     = "Tealium Collect for Ruby"
  s.description = %q{Add the Tealium gem to track activity and send data using Tealium's HTTP API}
  s.files = ["lib/tealium.rb", "lib/tealium_collect.rb"]
  s.require_paths = ["lib"]
  s.homepage    =
    'https://github.com/Tealium/tealium-ruby'
  s.license       = 'MIT'
  s.required_ruby_version = '2.3.4'
end
